<SCRIPT name="update(OSC_ARGS)" script="// local vars
decl trackName;
decl trackObject;
decl oscMsgIdx = 0;
decl found = 0;

// parse all objects
trackObject = getfirst(TRACK);

while(trackObject)
{
		// check all OSC names
		for(oscMsgIdx = 0; oscMsgIdx < sizeof(OSC_ARGS); oscMsgIdx++)
		{
				// current channel name
				trackName = OSC_ARGS[oscMsgIdx];
				
				// compare
				if(trackName == getexpression(trackObject, 'name'))
				{
						// set the name and the text if debug is on
						setexpression(trackObject, 'index', oscMsgIdx);
						setattribute(trackObject, 'color', RGB(0,1,0));
						found = 1;
						break;
				}
		}


		if(found == 0)
		{
				setexpression(trackObject, 'index', -1);	
				setattribute(trackObject, 'color', RGB(1,0,0));
		}

		// reset flag
		found = 0;

		// check name
		trackObject = getnext(trackObject);
}

" trigger_script="" trigger_type="2" trigger="1" osc_message="/live/name/trackblock" midi_message="0x90,0x90,0,0" midi_target="-1" flag="9"/>
<SCRIPT name="requestTrackNames(OSC_ARGS)" script="// num tracks
decl args = {0, OSC_ARGS[0]};

// get track block
oscout(0, '/live/name/trackblock', args);" trigger_script="" trigger_type="2" trigger="1" osc_message="/live/tracks" midi_message="0x90,0x90,0,0" midi_target="-1" flag="9"/>
<SCRIPT name="getIndexByName(trackName)" script="// check all elements and return right index
decl track = getfirst(TRACK);
while(track)
{
		if(getexpression(track, 'name') == trackName)
		{
				return getexpression(track, 'index');
		}
		track = getnext(track);
}
return -1;" trigger_script="" trigger_type="4" trigger="1" osc_message="/IDX/TRACK/getIndexByName" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
<SCRIPT name="init()" script="// get total number of tracks
oscout(0, '/live/tracks', 'query');" trigger_script="" trigger_type="5" trigger="1" osc_message="/IDX/TRACK/init" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
</WINDOW>
