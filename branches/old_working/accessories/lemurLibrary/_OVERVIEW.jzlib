<SCRIPT name="updateMaster(OSC_ARGS)" script="// get variables
decl leftRight = OSC_ARGS[0];
decl level = OSC_ARGS[1];

// set the right meter
MASTER.x[leftRight] = level;

// update clipping
if(level > clipThreshold)
{
		MASTER_CLIP.x[0] = 1;
}" trigger_script="" trigger_type="2" trigger="1" osc_message="/live/master/meter" midi_message="0x90,0x90,0,0" midi_target="-1" flag="9"/>
<SCRIPT name="updateChannel(OSC_ARGS)" script="// get variables
decl channelIndex = OSC_ARGS[0];
decl leftRight = OSC_ARGS[1];
decl level = OSC_ARGS[2];

// check if level is clipping
decl clipping = level > clipThreshold;

// set value for the right meter
if(channelIndex == IDX.TRACK.BASE.index)				
{
		if(clipping) 		BASE_CLIP.x = 1;
		BASE.x[leftRight] = level;
}
else if(channelIndex == IDX.TRACK.FX.index)	
{
		if(clipping) 		FX_CLIP.x = 1;
		FX.x[leftRight] = level;
}
else if(channelIndex == IDX.TRACK.HATZ.index)	
{
		if(clipping) 		HATZ_CLIP.x = 1;
		HATZ.x[leftRight] = level;
}
else if(channelIndex == IDX.TRACK.KICK.index)	
{
		if(clipping) 		KICK_CLIP.x = 1;
		KICK.x[leftRight] = level;
}
else if(channelIndex == IDX.TRACK.LEAD.index)	
{
		if(clipping) 		LEAD_CLIP.x = 1;
		LEAD.x[leftRight] = level;
}
else if(channelIndex == IDX.TRACK.SNR.index)	
{
		if(clipping) 		SNR_CLIP.x = 1;
		SNR.x[leftRight] = level;
}
else if(channelIndex == IDX.TRACK.SYNTH.index)	
{
		if(clipping) 		SYNTH_CLIP.x = 1;
		SYNTH.x[leftRight] = level;
}
else if(channelIndex == IDX.TRACK.TEX.index)	
{
		if(clipping) 		TEX_CLIP.x = 1;
		TEX.x[leftRight] = level;
}" trigger_script="" trigger_type="2" trigger="1" osc_message="/live/track/meter" midi_message="0x90,0x90,0,0" midi_target="-1" flag="9"/>
<SCRIPT name="init()" script="// save all channel colors
BASE.originalColor = getattribute(BASE, 'color');
setattribute(BASE_VOL, 'color', BASE.originalColor);

FX.originalColor = getattribute(FX, 'color');
setattribute(FX_VOL, 'color', FX.originalColor);

HATZ.originalColor = getattribute(HATZ, 'color');
setattribute(HATZ_VOL, 'color', HATZ.originalColor);

KICK.originalColor = getattribute(KICK, 'color');
setattribute(KICK_VOL, 'color', KICK.originalColor);

LEAD.originalColor = getattribute(LEAD, 'color');
setattribute(LEAD_VOL, 'color', LEAD.originalColor);

SNR.originalColor = getattribute(SNR, 'color');
setattribute(SNR_VOL, 'color', SNR.originalColor);

SYNTH.originalColor = getattribute(SYNTH, 'color');
setattribute(SYNTH_VOL, 'color', SYNTH.originalColor);

TEX.originalColor = getattribute(TEX, 'color');
setattribute(TEX_VOL, 'color', TEX.originalColor);

// and set master volume marker
setattribute(MASTER_VOL, 'color', getattribute(MASTER, 'color'));" trigger_script="" trigger_type="5" trigger="1" osc_message="/LEVEL_METERS/init" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
<SCRIPT name="resetClipLEDs()" script="// reset clipping sometimes
if((time % 4))
{
		if(BASE.x[0] < clipThreshold 	&& BASE.x[1] < clipThreshold && 	BASE_CLIP.x > 0)		BASE_CLIP.x -= clipAttenuation;
		if(FX.x[0] < clipThreshold 		&& FX.x[1] < clipThreshold && 		FX_CLIP.x > 0)			FX_CLIP.x -= clipAttenuation;
		if(HATZ.x[0] < clipThreshold 	&& HATZ.x[1] < clipThreshold && 	HATZ_CLIP.x > 0)		HATZ_CLIP.x -= clipAttenuation;
		if(KICK.x[0] < clipThreshold 	&& KICK.x[1] < clipThreshold &&		KICK_CLIP.x > 0)		KICK_CLIP.x -= clipAttenuation;
		if(LEAD.x[0] < clipThreshold 	&& LEAD.x[1] < clipThreshold &&		LEAD_CLIP.x > 0)		LEAD_CLIP.x -= clipAttenuation;
		if(SNR.x[0] < clipThreshold 	&& SNR.x[1] < clipThreshold &&		SNR_CLIP.x > 0)			SNR_CLIP.x -= clipAttenuation;
		if(SYNTH.x[0] < clipThreshold 	&& SYNTH.x[1] < clipThreshold && 	SYNTH_CLIP.x > 0)		SYNTH_CLIP.x -= clipAttenuation;
		if(TEX.x[0] < clipThreshold 	&& TEX.x[1] < clipThreshold &&		TEX_CLIP.x > 0)			TEX_CLIP.x -= clipAttenuation;
		if(MASTER.x[0] < clipThreshold 	&& MASTER.x[1] < clipThreshold && 	MASTER_CLIP.x > 0)		MASTER_CLIP.x -= clipAttenuation;
}" trigger_script="" trigger_type="1" trigger="1" osc_message="/live/play" midi_message="0x90,0x90,0,0" midi_target="-1" flag="9"/>
<SCRIPT name="getObjectByIdx(idx)" script="/*
		index				object

		0					level meter 
		1					volume marker
		2					clipping indicator

*/

// return the right level meter object
if(idx == IDX.TRACK.BASE.index)				return {BASE, BASE_VOL, BASE_CLIP};
else if(idx == IDX.TRACK.FX.index)			return {FX, FX_VOL, FX_CLIP};
else if(idx == IDX.TRACK.HATZ.index)		return {HATZ, HATZ_VOL, HATZ_CLIP};
else if(idx == IDX.TRACK.KICK.index)		return {KICK, KICK_VOL, KICK_CLIP};
else if(idx == IDX.TRACK.LEAD.index)		return {LEAD, LEAD_VOL, LEAD_CLIP};
else if(idx == IDX.TRACK.SNR.index)			return {SNR, SNR_VOL, SNR_CLIP};
else if(idx == IDX.TRACK.SYNTH.index)		return {SYNTH, SYNTH_VOL, SYNTH_CLIP};
else if(idx == IDX.TRACK.TEX.index)			return {TEX, TEX_VOL, TEX_CLIP};
" trigger_script="" trigger_type="4" trigger="1" osc_message="/LEVEL_METERS/getObjectByIdx" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
<SCRIPT name="updateVol(lvlObj, volObj, vol)" script="// show or hide
decl show = vol < volumeThresholdUpper & vol > volumeThresholdLower;

// calculate lowering
decl lowering = (vol / 0.85) * 0.975;

// show, hide and move
show(volObj, show);
decl meterRect = getobjectrect(lvlObj);
decl volRect = getobjectrect(volObj); 
volRect[1] = (meterRect[1] + meterRect[3] - volumeBorderOffset ) - meterRect[3]  * lowering ;
setobjectrect(volObj, volRect);
" trigger_script="" trigger_type="4" trigger="1" osc_message="/LEVEL_METERS/updateVol" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
<SCRIPT name="updateChannelVol(OSC_ARGS)" script="// get variables
decl channelIndex = OSC_ARGS[0];
decl volume = OSC_ARGS[1];

// get right objects
decl objects = getObjectByIdx(channelIndex);
decl objectVOL = objects[1];
decl objectLVL = objects[0];

// update volume marker
updateVol(objectLVL, objectVOL, volume);
" trigger_script="" trigger_type="2" trigger="1" osc_message="/live/volume" midi_message="0x90,0x90,0,0" midi_target="-1" flag="9"/>
<SCRIPT name="updateMasterVol(OSC_ARGS)" script="// get variables
decl volume = OSC_ARGS[0];

// update volume marker
updateVol(MASTER, MASTER_VOL, volume);
