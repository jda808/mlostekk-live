<SCRIPT name="receiveSceneName(OSC_ARGS)" script="// get osc args
decl index = OSC_ARGS[0];
decl name = OSC_ARGS[1];

// safe in array
sceneNames[index] = name;

// update pads
updateSceneLabels(currentScene);

// if its the first one, set
//if" trigger_script="" trigger_type="2" trigger="1" osc_message="/live/name/scene" midi_message="0x90,0x90,0,0" midi_target="-1" flag="9"/>
<SCRIPT name="updateSceneLabels(OSC_ARGS)" script="// check if we already got the scene names
if(sizeof(sceneNames) == 0)
{
		requestAllSceneNames();
		return;
}

// osc args
currentScene = OSC_ARGS[0] - 1;

// locals
decl index;

// before current
decl sizeBefore = getattribute(SCENES_BEFORE, 'row');
decl arrayBefore;
if(sizeBefore > currentScene)
{		
		arrayBefore = fill(1, '-------', sizeBefore - currentScene);
		decl startIndex = max(currentScene - sizeBefore, 0);		
		decl length = min(sizeBefore, sizeBefore - currentScene + 1);
		arrayBefore = {arrayBefore, subarray(sceneNames, startIndex, length)};	
}
else
{
		arrayBefore = subarray(sceneNames, currentScene - sizeBefore, sizeBefore);
}
setattribute(SCENES_BEFORE, 'labels', arrayBefore);

// set current scene name
setattribute(SCENE_NOW, 'labels', sceneNames[currentScene]);

// after current
decl sizeAfter = getattribute(SCENES_AFTER, 'row');
decl arrayAfter;
if(currentScene + sizeAfter >= sizeof(sceneNames))
{		
		arrayAfter = fill(1, '-------', (currentScene + sizeAfter + 1) - sizeof(sceneNames));	
		decl startIndex = currentScene + 1;
		decl length = sizeof(sceneNames) - startIndex;
		if(length > 0)
		{
				arrayAfter = {subarray(sceneNames, startIndex, length), arrayAfter};	
		}
}
else
{
		arrayAfter = subarray(sceneNames, currentScene + 1, sizeAfter);
}
setattribute(SCENES_AFTER, 'labels', arrayAfter);" trigger_script="" trigger_type="2" trigger="1" osc_message="/live/scene" midi_message="0x90,0x90,0,0" midi_target="-1" flag="9"/>
