oscout(0, '/live/name/scene', 'query');
" trigger_script="" trigger_type="4" trigger="1" osc_message="/SCENE_INFO/requestAllSceneNames" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
</WINDOW>
</WINDOW>
